#include <stdio.h>
#include <math.h>

int main() {

int a, b, c;

for (a; a <= 1000; a++){
  for (b; b <= 1000; b++){
    for (c; c <= 1000; c++){

      if (a*a+b*b == c*c || a*a+c*c == b*b || b*b+c*c == a*a){
	if (a+b+c <= 1000){
	  printf("Trojkat o bokach %d %d %d.", a, b, c);
	}
      }

    }
  }
}


return 0;
}
